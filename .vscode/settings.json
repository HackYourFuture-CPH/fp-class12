{
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "editor.formatOnSave": true,
  "prettier.configPath": "./.prettierrc.js",
  "spellright.language": [
    "en"
  ],
  "spellright.documentTypes": [
    "markdown",
    "latex",
    "plaintext",
    "ignore"
  ],
  "spellright.parserByClass": {
    "ignore": {
      "parser": "plain"
    }
  }
}
